    "@types/react-dom": "^18.0.10",
    "@types/react-is": "^17.0.3",
    "@types/validator": "^10.11.3",
    "clsx": "^1.0.0",
    "cross-env": "^5.1.4",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "gh-pages": "^1.2.0",
    "mobx": "^6.0.0",
    "mobx-react": "^7.0.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "rollup": "^3.8.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-delete": "^2.0.0",
